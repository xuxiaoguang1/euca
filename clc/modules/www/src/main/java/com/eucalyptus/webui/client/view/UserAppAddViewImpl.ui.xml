<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
	<table width="100%" hight="100%">
		<tr>
		<td width="50%" align="center">
			<g:LayoutPanel height="190px" width="300px">
				<g:layer left="5px" width="110px" top="15px" height="30px">
					<g:Label text="服务开始时间"/>
				</g:layer>
				<g:layer left="120px" width="190px" top="15px" height="30px">
					<p1:DateBox ui:field="startingTime"/>
				</g:layer>
				<g:layer left="5px" width="110px" top="50px" height="30px">
					<g:Label text="服务结束时间"/>
				</g:layer>
				<g:layer left="120px" width="190px" top="50px" height="30px">
					<p1:DateBox ui:field="endingTime"/>
				</g:layer>
				<g:layer left="14px" width="110px" top="85px" height="30px">
					<g:Label text="虚拟机镜像"/>
				</g:layer>
				<g:layer left="120px" width="190px" top="85px" height="30px">
					<g:ListBox ui:field="VMImageTypeList"/>
				</g:layer>
				<g:layer left="5px" width="110px" top="120px" height="30px">
					<g:Label text="Keypair"/>
				</g:layer>
				<g:layer left="120px" width="190px" top="120px" height="30px">
					<g:ListBox ui:field="keyPairList"/>
				</g:layer>
				<g:layer left="5px" width="110px" top="155px" height="30px">
					<g:Label text="安全组"/>
				</g:layer>
				<g:layer left="120px" width="190px" top="155px" height="30px">
					<g:ListBox ui:field="securityGroupList"/>
				</g:layer>
			</g:LayoutPanel>
		</td>
		<td width="50%" align="center">
				<g:LayoutPanel height="190px" width="300px">
					<g:layer left="5px" width="110px" top="15px" height="30px">
						<g:Label text="模板"/>
					</g:layer>
					<g:layer left="120px" width="190px" top="15px" height="30px">
						<g:ListBox ui:field="TemplateList"/>
					</g:layer>
					<g:layer left="5px" width="110px" top="50px" height="30px">
						<g:Label text="CPU"/>
					</g:layer>
					<g:layer left="120px" width="190px" top="50px" height="30px">
						<g:TextBox ui:field="template_ncpus" />
					</g:layer>
					<g:layer left="5px" width="110px" top="85px" height="30px">
						<g:Label text="内存"/>
					</g:layer>
					<g:layer left="120px" width="190px" top="85px" height="30px">
						<g:TextBox ui:field="template_mem" />
					</g:layer>
					<g:layer left="5px" width="110px" top="120px" height="30px">
						<g:Label text="磁盘"/>
					</g:layer>
					<g:layer left="120px" width="190px" top="120px" height="25px">
						<g:TextBox ui:field="template_disk" />
					</g:layer>
					<g:layer left="5px" width="110px" top="155px" height="30px">
						<g:Label text="带宽"/>
					</g:layer>
					<g:layer left="120px" width="190px" top="155px" height="30px">
						<g:TextBox ui:field="template_bw" />
					</g:layer>
				</g:LayoutPanel>
		</td>
		</tr>
		<tr>
			<td colspan="2" align="right">
				<g:LayoutPanel width="818px" height="55px">
					<g:layer left="693px" width="60px" top="16px" height="30px">
						<g:Button text="确定" ui:field="buttonOk"/>
					</g:layer>
					<g:layer left="625px" width="60px" top="16px" height="30px">
						<g:Button text="取消" ui:field="buttonCancle"/>
					</g:layer>
				</g:LayoutPanel>
			</td>
		</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 